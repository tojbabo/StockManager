<Window x:Class="StockManager.VIEW.FRAMEMAIN"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:StockManager.VIEW"
        mc:Ignorable="d"
        Title="MAINFRAME" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="7*"/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0" Background="Blue">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="6*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
                <ColumnDefinition Width="*"/>
            </Grid.ColumnDefinitions>
            <Button Grid.Column="2" Content="참고용" Click="Btn_Input"/>
            <Button Grid.Column="3" Content="재료" Click="Btn_Material"/>
            <Button Grid.Column="4" Content="stuff_temp" Click="Btn_Stuff"/>
        </Grid>
        <Frame Grid.Row="1" x:Name="FRAME" NavigationUIVisibility="Hidden"/>
    </Grid>
</Window>
